{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission",
    "description": "Submissions from the contact form"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "subject": {
      "type": "string"
    },
    "message": {
      "type": "text",
      "required": true
    },
    "isRead": {
      "type": "boolean",
      "default": false
    },
    "submittedAt": {
      "type": "datetime",
      "required": true
    }
  }
}